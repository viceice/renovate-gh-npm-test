{
  "extends": [
    "config:base",
    ":masterIssue"
  ],
  "encrypted": {
    "npmToken": ""
  },
  "npmrc": "registry=https://npm.pkg.github.com/viceice\n//npm.pkg.github.com/viceice:_authToken=${NPM_TOKEN}",
  "github-actions": { "enabled": true } ,
  "regexManagers": [
    {
      "fileMatch": ["^_\\.github/workflows/[^/]+\\.ya?ml$"],
      "matchStrings": [
        "uses:\\s(?<depName>.*?)@(?<currentValue>.*)\\s"
      ],
      "datasourceTemplate": "github-tags"
    }
  ]
}
